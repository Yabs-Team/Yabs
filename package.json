{
  "dependencies": {
    "docsify": "^4.12.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && cd frontend && yarn run test:unit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --cache --max-warnings=0 -c ./frontend/.eslintrc.js --fix"
    ]
  }
}
